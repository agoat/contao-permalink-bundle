{
    "name": "agoat/contao-permalink",
    "description": "Contao Permalink Extension",
    "keywords": ["contao", "contao4", "contao-bundle", "plugin", "permalink", "url", "hyperlink", "cms"],
    "type": "contao-bundle",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Arne Stappen (alias aGoat)",
            "homepage": "https://agoat.xyz"
        }
    ],
    "require": {
        "php": "^7.0",
        "symfony/framework-bundle": "~2.8|~3.0",
        "contao/core-bundle": "^4.4.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5"
    },
    "autoload": {
        "psr-4": {
            "Agoat\\PermalinkBundle\\": "src/"
        },
        "classmap": [
            "src/Resources/contao"
        ],
        "exclude-from-classmap": [
            "src/Resources/contao/config",
            "src/Resources/contao/dca",
            "src/Resources/contao/languages",
            "src/Resources/contao/templates"
        ]
    },
    "extra": {
        "contao-manager-plugin": "Agoat\\PermalinkBundle\\ContaoManager\\Plugin"
    }
}
